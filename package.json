{
  "name": "@elizaos/eliza-starter",
  "version": "0.1.9",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "dev": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "deploy": "pnpm clean && pnpm i && pnpm start",
    "clean": "./scripts/clean.sh",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all"
  },
  "dependencies": {
    "@cetusprotocol/aggregator-sdk": "^0.3.21",
    "@elizaos/adapter-postgres": "^0.1.9",
    "@elizaos/adapter-sqlite": "0.1.9",
    "@elizaos/client-auto": "0.1.9",
    "@elizaos/client-direct": "0.1.9",
    "@elizaos/client-twitter": "0.1.9",
    "@elizaos/core": "0.1.9",
    "@elizaos/plugin-bootstrap": "0.1.9",
    "@elizaos/plugin-image-generation": "0.1.9",
    "@elizaos/plugin-node": "0.1.8",
    "@mysten/sui": "^1.16.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tavily/core": "0.0.2",
    "amqplib": "0.10.5",
    "axios": "^1.7.9",
    "better-sqlite3": "11.6.0",
    "bignumber.js": "9.1.2",
    "bn.js": "^5.2.1",
    "fs": "0.0.1-security",
    "net": "1.0.2",
    "node-cache": "5.1.2",
    "path": "0.12.7",
    "pg": "^8.13.1",
    "prettier": "^3.4.2",
    "readline": "1.3.0",
    "tsup": "8.3.5",
    "url": "0.11.4",
    "uuid": "^11.0.5",
    "vitest": "2.1.4",
    "ws": "8.18.0",
    "yargs": "17.7.2",
    "zod": "3.23.8"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0"
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "nodemon": "3.1.9",
    "pm2": "5.4.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  }
}
